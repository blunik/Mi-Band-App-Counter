{
  "package": "com.application.cutecounter", 
  "name": "Counter",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1000,
  "icon": "/common/logo_new.png",
  "deviceTypeList": [
    "watch"
  ],
  "features": [
    {
      "name": "system.router"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.prompt"
    },
    {
      "name": "system.vibrator"
    }
  ],
  "config": {
    "logLevel": "log",
    "designWidth": "device-width"
  },
  "display": {
    "backgroundColor": "#000000"
  },
  "router": {
    "entry": "pages/list",
    "pages": {
      "pages/list": {
        "component": "list"
      },
      "pages/detail": {
        "component": "detail"
      }
    }
  }
}